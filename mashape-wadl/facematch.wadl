                    <representation element="f2117:root" mediaType="application/json"/>
                </response>
            </method>
        </resource>
        <resource uri="random/">
            <method displayName="Random female image" id="2115" name="GET">
                <doc>Returns a random female K-pop star image.</doc>
                <request/>
            </method>
            <method displayName="Random Image" id="2116" name="GET">
                <doc>Returns a random K-pop star picture.</doc>
                <request/>
            </method>
            <method displayName="Random male image" id="2118" name="GET">
                <doc>Returns a random male K-pop star image.</doc>
                <request/>
            </method>
