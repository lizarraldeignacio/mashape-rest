        <resource uri="associations/">
            <method displayName="Word Associations (get)" id="3174" name="GET">
                <doc>Get word associations with semantic distance score.</doc>
                <request>
                    <param name="entry" required="true" style="query" type="xsd:string">
                        <doc>a word phrase or paragraph</doc>
                    </param>
                </request>
            </method>
            <method displayName="Word Associations (post)" id="3175" name="POST">
                <doc>Get word associations with semantic distance score.</doc>
                <request/>
                <response>
                    <representation element="f3175:root" mediaType="application/json"/>
                </response>
            </method>
        </resource>
    </resources>
</application>
