                            <doc>xml or json</doc>
                        </param>
                        <param name="wfsalphabet" required="true" style="query" type="xsd:string">
                            <doc>filter results by fonts starting with this letter a or b or z</doc>
                        </param>
                        <param name="wfsclassid" required="true" style="query" type="xsd:string">
                            <doc>filter results by this classification ID</doc>
                        </param>
                        <param name="wfscss" required="true" style="query" type="xsd:string">
                            <doc>provide the font face rule for each font when set to</doc>
                        </param>
                        <param name="wfsdesignerid" required="true" style="query" type="xsd:string">
                            <doc>filter results by this designer ID</doc>
                        </param>
