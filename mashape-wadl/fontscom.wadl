                            <doc>filter results by this foundry ID</doc>
                        </param>
                        <param name="wfsfree" required="true" style="query" type="xsd:boolean">
                            <doc>filter results by fonts which are free only when set to true</doc>
                        </param>
                        <param name="wfskeyword" required="true" style="query" type="xsd:string">
                            <doc>keyword to be searched by</doc>
                        </param>
                        <param name="wfslangid" required="true" style="query" type="xsd:string">
                            <doc>filter results by this language ID</doc>
                        </param>
                        <param name="wfsplimit" required="true" style="query" type="xsd:integer">
                            <doc>of records requested</doc>
                        </param>
                        <param name="wfspstart" required="true" style="query" type="xsd:string">
                            <doc>starting record requested zero based</doc>
                        </param>
                    </request>
                </method>
