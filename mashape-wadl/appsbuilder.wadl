                            <param name="id" required="true" style="query" type="xsd:string">
                                <doc>this is the app ID</doc>
                            </param>
                        </request>
                        <response>
                            <representation element="f806:root" mediaType="application/json"/>
                        </response>
                    </method>
                </resource>
            </resource>
        </resource>
        <resource uri="ad/">
