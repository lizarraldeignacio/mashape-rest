                <request/>
                <response>
                    <representation element="f4103:root" mediaType="application/json"/>
                </response>
            </method>
        </resource>
        <resource uri="drill/">
            <resource uri="{drill_id}/">
                <param name="drill_id" style="template" type="xsd:string"/>
